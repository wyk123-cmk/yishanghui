page{
	max-width: 750px;
	margin: 0 auto;
}
.page-bg {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	overflow-y: auto;
	background-color: #f7f7f7;
	overflow-x: hidden;
}
view,
button,
input,
textarea{
	color: #333;
	font-size: 14px;
}
button::after{
	border: 0;
}
uni-toast .uni-toast{
	width: 5em;
}
uni-toast .uni-toast__content{
	font-size: 14px;
}
._h2{
	font-size: 14px;
}
._a{
	text-decoration: none;
}
._p {
    font-size: 14px;
    line-height: 22px;
	padding-bottom: 6px;
}
.tkbt{
    font-size: 16px;
    display: block;
	margin-bottom: 5px;
    text-align: center;
    color: #000000;
    font-weight: bold;
}
.fuwutiaok-hz {
    color: #FF003A;
    font-weight: bold;
}
.fuwutiaok-hzxhx{
	color: #FF0000;
}
.fuwutiaok-hcz{
	padding-top: 6px;
	font-weight: bold;
}
.fuwutiaok-hzxhx,
.fuwutiaok-hczxhx{
    text-decoration: underline;
    font-weight: bold;
	font-size: 14px;
}
checkbox .wx-checkbox-input{
	-webkit-transform:scale(0.6);
	        transform:scale(0.6);
}
.checkbox-radio checkbox .wx-checkbox-input {
	border-radius: 50%;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked{
	background-color: #4285f4;
	color: #FFFFFF !important;
	border-color: #4285f4 !important;
}
checkbox.orange .wx-checkbox-input.wx-checkbox-input-checked{
	background-color: #FD9523;
	color: #FFFFFF !important;
	border-color: #FD9523 !important;
}
switch .wx-switch-input{
	height: 24px;
}
switch .wx-switch-input:after, 
switch .wx-switch-input:before{
	height: 22px;
}
radio[disabled] .wx-radio-input{
	background-color: #F0F0F0 !important;
}
.navigator-hover{
	background-color: transparent;
	opacity: 1;
}
.transport-o{
	display: -webkit-box;
	display: flex;
	display: -webkit-flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	padding: 18px 0;
}
.transport-o text{
	display: block;
}
.transport-o .status,
.transport-o .express{
	width: 33%;
	text-align: center;
}
.transport-o .express{
	font-size: 16px;
}
.transport-o .express .city{
	font-size: 14px;
	color: #666;
	padding-bottom: 5px;
}
.transport-o .status{
	font-size: 12px;
	color: #999999;
}
.transport-o .status image{
	width: 90px;
	height: 4px;
	padding-bottom: 5px;
}
.none-data-list{
	text-align: center;
	color: #999;
	padding-top: 30%;
}
.none-data-list image{
	display: block;
	margin: 0 auto 5px;
	width: 75px;
	height: 76px;
}
/* button样式 */
.btn-default-css,
.btn-fixed-bottom{
	background-color: #4285F4;
	color: #fff;
}
.btn-default-css{
	height: 40px;
	line-height: 40px;
	margin: 20px 15px;
}
.btn-fixed-bottom{
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	height: 46px;
	line-height: 46px;
	border-radius: 0;
}
/*右上角*/
.head-right{
	position: fixed;
	z-index: 998;
}
.head-right{
	top: 10px;
	right: 0;
	color: #fff;
	padding: 3px 10px;
	border-top-left-radius: 15px;
	border-bottom-left-radius: 15px;
	background-color: #4285f4;
}
.head-right.orange{
	background-color: #ff7742;
}
.share-popup{
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	z-index: 1000;
	text-align: center;
}
.v-model{
	position: fixed;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	opacity: .5;
	background: #000;
	z-index: 999;
}
.input-view{
	background-color: #F0AD4E;
	position: absolute;
	top: 0;
	bottom: 0;
	z-index: 1;
	opacity: 0;
}

