<view class="page-bg data-v-4d91f128"><navigator class="head-right data-v-4d91f128" url="/pages/me/historicalInvoice">历史开票</navigator><block wx:if="{{total==0}}"><view class="none-data-list data-v-4d91f128"><image src="../../static/images/none.svg" class="data-v-4d91f128"></image>暂无内容</view></block><block wx:else><view class="data-v-4d91f128"><scroll-view class="scroll-wrapper data-v-4d91f128" scroll-y="true" data-event-opts="{{[['scrolltolower',[['scrolltolower',['$event']]]]]}}" bindscrolltolower="__e"><block wx:for="{{invoiceData}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view class="invoice-item-list data-v-4d91f128"><checkbox-group data-event-opts="{{[['change',[['checkboxChange',['$event','$0','$1'],[[['invoiceData','',i,'price']],[['invoiceData','',i,'mastCode']]]]]]]}}" class="checkbox-radio data-v-4d91f128" bindchange="__e"><checkbox value="{{item.mastCode}}" checked="{{item.checked}}" class="data-v-4d91f128"></checkbox><text class="data-v-4d91f128">{{"运单号："+item.expressCode}}</text></checkbox-group><view data-event-opts="{{[['tap',[['nextStep',[i]]]]]}}" class="transport-o data-v-4d91f128" bindtap="__e"><view class="express data-v-4d91f128"><text class="city data-v-4d91f128">{{item.expressFrom.areaInfo.city}}</text><text class="data-v-4d91f128">{{item.expressFrom.name}}</text></view><view class="status data-v-4d91f128"><image mode="widthFix" src="../../static/images/cjt.svg" class="data-v-4d91f128"></image></view><view class="express data-v-4d91f128"><text class="city data-v-4d91f128">{{item.expressTo.areaInfo.city}}</text><text class="data-v-4d91f128">{{item.expressTo.name}}</text></view><view class="price data-v-4d91f128">{{"￥"+item.price}}</view></view></view></block></scroll-view><view class="invoice-footer data-v-4d91f128"><checkbox-group data-event-opts="{{[['change',[['checkAllChange',['$event']]]]]}}" class="checkbox-radio data-v-4d91f128" bindchange="__e"><checkbox checked="{{isCheckAll}}" class="data-v-4d91f128"></checkbox><text class="data-v-4d91f128">全选</text></checkbox-group><button data-event-opts="{{[['tap',[['nextStep']]]]}}" class="next-step data-v-4d91f128" bindtap="__e">下一步</button><view class="amount data-v-4d91f128">合计：<text class="data-v-4d91f128">{{totalSum==0?'--':totalSum}}</text></view></view></view></block></view>